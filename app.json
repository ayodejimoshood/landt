{
  "expo": {
    "name": "Woozeee",
    "slug": "woozeee-app",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/woozeee-icon.png",
    "splash": {
      "image": "./assets/splash-bg.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.bahbi.twitterclone",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.bahbi.twitterclone",
      "versionCode": 1,
      "useNextNotificationsApi": true
    }
  }
}